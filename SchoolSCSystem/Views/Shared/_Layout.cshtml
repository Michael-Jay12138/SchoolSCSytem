            float: left;
            background: #fff;
            color: #659be0;
            font: 14px/1.5 微软雅黑;
            text-align: center;
        }

            #weather i {
                float: left;
                background: no-repeat top left;
                height: 68px;
                line-height: 68px;
                font-size: 40px;
                padding-left: 80px;
                font-style: normal;
                text-align: center;
                font-weight: bold;
            }

                #weather i.icon-xiaoyu {
                    background-image: url(/Content/weather/icon/xiaoyu.png);
                }

                #weather i.icon-zhongyu {
                    background-image: url(/Content/weather/icon/zhongyu.png);
                }

                #weather i.icon-dayu {
                    background-image: url(/Content/weather/icon/dayu.png);
