                    else {
                        $('.dropdown-user').find('ul').html("<li><a href='/Client/Views/Login/index.html'><i class='icon-key'></i> 登录</a></li>");
                    }
                },
                getCookie: function (name) {
                    var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
                    if (arr = document.cookie.match(reg))
                        return unescape(arr[2]);
                    else
                        return null;
                },
                delCookie: function (name) {
                    var exp = new Date();
                    exp.setTime(exp.getTime() - 1);
                    var cval = getCookie(name);
                    if (cval != null)
                        document.cookie = name + "=" + cval + ";expires=" + exp.toGMTString() + ";path=/";
                },
                initialParentMenu: function (data) {
                    var _html = "";
                    $.each(data, function (i) {
                        var row = data[i];
                        if (row.ParentId == 0) {
                            _html += '<li class="nav-item"><a href="javascript:;" class="nav-link nav-toggle"><i class="' + row.Icon + '"></i><span class="title">' + row.MenuName + '</span><span class="selected"></span><span class="arrow open"></span></a><ul class="sub-menu" id=' + row.MenuId + '></ul></li>';
                        }
                    })
                    $("#accordion").prepend(_html);
                },
                initialSubMenu: function (data) {
                    $.each(data, function (i) {
                        var row = data[i];
                        var _html = "";
                        if (row.ParentId != 0) {
                            if (row.PageUrl == '无')
                                _html += '<li class="nav-item  "><a href="javascript:;" class="nav-link nav-toggle"><span class="title">' + row.MenuName + '</span><span class="arrow"></span></a><ul class="sub-menu" id=' + row.MenuId + '></ul></li>';
                            else
                                _html += '<li class="nav-item  "><a href="' + row.PageUrl + '" class="nav-link "><span class="title">' + row.MenuName + '</span></a></li>';
                        }
                        $("#" + row.ParentId).prepend(_html);
                    })
                },
                initialDetail: function (data) {
                    var _html = "";
                    $.each(data, function (l) {
                        var row = data[l];
                        if (row.approvalType == "预定教室") {
                            _html += '<li><a href="ApprovalData#/approvalData/confirm/' + row.approvalId + '"><span class="photo"><img src="/Content/assets/layouts/layout2/img/' + row.creatorAvatar + '.jpg" class="img-circle" alt=""></span><span class="subject"><span class="from">申请人： ' + row.creatorName + ' </span><span class="time">16 mins </span></span><span class="message">申请教室： ' + row.objectName + ' </span></a> </li>';
                        }
                    })
                    $("#classroomDropDetails").prepend(_html);
                    _html = "";
                    $.each(data, function (m) {
                        var row = data[m];
                        if (row.approvalType == "选课") {
                            _html += '<li><a href="ApprovalData#/approvalData/confirm/' + row.approvalId + '"><span class="photo"><img src="/Content/assets/layouts/layout2/img/' + row.creatorAvatar + '.jpg" class="img-circle" alt=""></span><span class="subject"><span class="from"> 申请人： ' + row.creatorName + ' </span><span class="time">16 mins </span></span><span class="message">选择课程： ' + row.objectName + ' </span></a> </li>';
                        }
                    })
                    $("#courseDropDetails").prepend(_html);
                    _html = "";
                    $.each(data, function (n) {
                        var row = data[n];
                        if (row.approvalType == "请假") {
                            _html += '<li><a href="ApprovalData#/approvalData/confirm/' + row.approvalId + '"><span class="photo"><img src="/Content/assets/layouts/layout2/img/' + row.creatorAvatar + '.jpg" class="img-circle" alt=""></span><span class="subject"><span class="from">申请人：  ' + row.creatorName + ' </span><span class="time">16 mins </span></span><span class="message">请假原因： ' + row.objectName + ' </span></a> </li>';
                        }
                    })
                    $("#leaveDropDetails").prepend(_html);
                },
                loadDetails: function () {
                    var userid = this.getCookie("userid");
                    $.get("/api/ApprovalData/GetObjectDetail?userId=" + userid).then(function (result) {
                        $.get("/api/ApprovalData/GetObjectsNum?userId=" + userid).then(function (result_o) {
                            var jsonstr = "[{";
                            for (i = 0; i < result_o.length; i++) {
                                jsonstr += i + ":" + "\"" + result_o[i] + "\",";
                            }
                            jsonstr = jsonstr.substring(0, jsonstr.lastIndexOf(','));
                            jsonstr += "}]";
                            var jsonobj = eval(jsonstr);
                            $('#coursenum').text(jsonobj[0][0]);
                            $('#leavenum').text(jsonobj[0][1]);
                            $('#classroom_usernum').text(jsonobj[0][2]);
                        })
                        myFun.fn.initialDetail(result);
                    })
                },
                loadMenu: function () {
                    var userid = this.getCookie("userid");
                    $.get("/api/menu/GetMenusByUserId?userId=" + userid).then(function (result) {
                        myFun.fn.initialParentMenu(result);
                        myFun.fn.initialSubMenu(result);
                        myFun.fn.loadDetails();
                    })
                },
            });
            function makeArray(selector) {
                var arr = document.querySelectorAll(selector);
                return arr;
            }
            return myFun;
        })();

        $(window).load(function () {
            window.setTimeout(function () {
                $('#ajax-loader').fadeOut();
            }, 300);
        });

        $(document).ready(function () {
            $su.fn.getReady();
        })
    </script>
    @RenderSection("scripts", required: false)
    <!-- END THEME LAYOUT SCRIPTS -->
</body>
</html>
