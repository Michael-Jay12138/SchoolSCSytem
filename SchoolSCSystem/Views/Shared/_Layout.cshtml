                #weather i.icon-duoyun {
                    background-image: url(/Content/weather/icon/duoyun.png);
                }

                #weather i.icon-yin {
                    background-image: url(/Content/weather/icon/yin.png);
                }

            #weather p {
                float: right;
                font-size: 17px;
            }

                #weather p span {
                    margin: 0 15px;
                }
    </style>
