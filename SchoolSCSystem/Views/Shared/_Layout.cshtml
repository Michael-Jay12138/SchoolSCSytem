        var $su = (function () {
            var myFun = function (selector) {
                return makeArray(selector);
            }
            myFun.extend = function (target) {
                for (var i = 1; i < arguments.length; i++) {
                    for (var prop in arguments[i]) {
                        target[prop] = arguments[i][prop];
                    }
                }
                return target;
            }
            myFun.fn = myFun.extend(NodeList.prototype, {
                getReady: function () {
                    var name = this.getCookie("name");
                    if (name != null) {
                        $(".username").text(name);
                        $("#infocenter").attr("href", "User#/user/info/" + this.getCookie("userid"));
                        $.get("/api/user/GetUser?id=" + this.getCookie("userid")).then(function (result_u) {
                            $(".img-circle").attr("src", "/Content/assets/layouts/layout2/img/" + result_u.UserAvatar + ".jpg");
